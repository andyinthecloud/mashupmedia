<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:pro="http://www.liquibase.org/xml/ns/pro"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
                        http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-latest.xsd">


    <changeSet id="001-1" author="admin">
        <insert tableName="authorities">
            <column name="id" valueSequenceNext="authorities_seq" />
            <column name="id_name" value="role.admin" />
            <column name="name" value="Administrator" />
            <column name="authority" value="ROLE_ADMINISTRATOR" />
            <column name="created_on" defaultValueDate="${now}" />
        </insert>
    </changeSet>


</databaseChangeLog>                        